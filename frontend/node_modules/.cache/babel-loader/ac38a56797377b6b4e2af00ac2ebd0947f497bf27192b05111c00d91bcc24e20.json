{"ast":null,"code":"import{useState,useEffect}from'react';export function useRealData(fetchFunction){let autoFetch=arguments.length>1&&arguments[1]!==undefined?arguments[1]:true;const[data,setData]=useState(null);const[loading,setLoading]=useState(false);const[error,setError]=useState(null);const fetchData=async()=>{setLoading(true);setError(null);try{const result=await fetchFunction();setData(result);}catch(err){setError(err instanceof Error?err.message:'Erreur inconnue');}finally{setLoading(false);}};useEffect(()=>{if(autoFetch){fetchData();}},[autoFetch]);return{data,loading,error,refetch:fetchData};}","map":null,"metadata":{},"sourceType":"module","externalDependencies":[]}